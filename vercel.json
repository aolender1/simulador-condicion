{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    },
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/check-access",
      "dest": "/server.js"
    },
    {
      "src": "/api/events(.*)",
      "dest": "/server.js"
    },
    {
      "src": "/api/contacts(.*)",
      "dest": "/server.js"
    },
    {
      "src": "/api/send-email",
      "dest": "/server.js"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}